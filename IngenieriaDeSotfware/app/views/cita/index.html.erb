<p id="notice"><%= notice %></p>
<div align=center>
  <h1>Cita</h1>

  <table>
    <thead>
      <tr>
        <th>User</th>
        <th>Restaurant</th>
        <th>Date</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @cita.each do |citum| %>
        <tr>
          <td><%= citum.user_id %></td>
          <td><%= citum.restaurant_id %></td>
          <td><%= citum.date %></td>
          <td><%= link_to 'Show', citum %></td>
          <td><%= link_to 'Edit', edit_citum_path(citum) %></td>
          <td><%= link_to 'Destroy', citum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Citum', new_citum_path %>
  <%= link_to 'Home', home_path %>
</div>
